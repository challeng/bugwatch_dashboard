Welcome to bugwatch!

<% @presenter.files.each do |file, klasses| %>
  <%= file %>
  <% klasses.each do |klass, alerts| %>
    &nbsp;<%= klass %>
    <% alerts.each do |alert| %>
        &nbsp;&nbsp;<%= alert.function %><%= link_to "View alert", repo_alert_path(alert.commit.repo.id, alert.id) %>
    <% end %>
  <% end %>
<% end %>