<%= @repo.name %>

<%= form_for @subscription do |f| %>
    <%= f.label :notify_on_analysis %>
    <%= f.check_box :notify_on_analysis %>
    <%= f.submit %>
<% end %>

<%= render "complexity_chart", :commits => @commits, :repo => @repo %>
<%= render "fix_cache_chart", :hot_spots => @hot_spots %>

<%= link_to "View Alerts", repo_alerts_path(@repo) %>